	"tokenColors": [
		// General
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},


		// Keywords
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#6adc40da"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type.function",
				"storage.type.class",
				"storage.type.interface",
				"storage.type.trait"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#BEB2A0"
			}
		},
		{
			"name": "Methods",
			"scope": [
				"entity.name.function",
				"support.function.magic",
				"meta.function-call",
				"meta.method-call",
				"meta.use constant.other"
			],
			"settings": {
				"foreground": "#8fcbff"
			}
		},
		{
			"name": "Methods",
			"scope": [
				"meta.function entity.name.function",
			],
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"name": "Magic methods",
			"scope": [
				"meta.function support.function.magic",
			],
			"settings": {
				"fontStyle": "bold italic",
			}
		},


		// Types
		{
			"name": "Types",
			"scope": [
				"storage.type",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5affe6"
			}
		},
		{
			"name": "Namespaces",
			"scope": [
				"entity.name.type.namespace",
				"support.other.namespace",
				"support.other.namespace storage.type"
			],
			"settings": {
				"foreground": "#49a89a"
			}
		},
		{
			"name": "Namespaces",
			"scope": [
				"support.other.namespace variable.language.namespace",
			],
			"settings": {
				"foreground": "#eae188"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"support.class",
				"entity.other.inherited-class",
				"entity.other.alias",
				"meta.interface"
			],
			"settings": {
				"foreground": "#5affe6"
			}
		},
		{
			"name": "Class aliases",
			"scope": [
				"entity.other.alias"
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "Class names",
			"scope": [
				"entity.name.type.class",
				"entity.name.type.interface",
				"entity.name.type.trait",
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#a9d7ff"
			}
		},

		
		

		// Punctuation
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#868e90"
			}
		},
		{
			"name": "Subtle punctuation",
			"scope": [
				"punctuation.separator.inheritance",
			],
			"settings": {
				"foreground": "#868e9094"
			}
		},
		{
			"name": "Embedded",
			"scope": [
				"punctuation.section.embedded"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ea7b7b"
			}
		},
		{
			"name": "Type operators",
			"scope": [
				"keyword.operator.type",
				"keyword.other.use-as",
				"keyword.other.use-insteadof"
			],
			"settings": {
				"foreground": "#eae188"
			}
		},

		


		// Constants
		{
			"name": "Constants",
			"scope": [
				"constant",
				"support.constant",
			],
			"settings": {
				"foreground": "#dbace8"
			}
		},
		{
			"name": "Language constants",
			"scope": [
				"constant.other"
			],
			"settings": {
				"foreground": "#fd7acd"
			}
		},
		{
			"name": "Bold constants",
			"scope": [
				"constant.other",
				"constant.character.escape"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},


		// Comments
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#60554a"
			}
		},
		{
			"name": "Comment keywords",
			"scope": [
				"comment keyword"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#fd7acde1"
			}
		},


		// Variables
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#ced4da"
			}
		},
		{
			"name": "Variable defs",
			"scope": [
				"punctuation.definition.variable",
			],
			"settings": {
				"foreground": "#a2a8af"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#8e262b"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},


		// Strings
		{
			"name": "Quotes",
			"scope": [
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#85625B"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted"
			],
			"settings": {
				"foreground": "#ff9494"
			}
		},


		// Declaratives
		{
			"name": "Keys",
			"scope": [
				"entity.name.tag.yaml",
				"entity.name.function.neon",
				"support.other.neon"
			],
			"settings": {
				"foreground": "#5affe6"
			}
		},
		


		// Markup
		{
			"name": "Tag begin / end",
			"scope": [
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
			],
			"settings": {
				"foreground": "#d2c4b5"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#b09b7c"
			}
		},
		{
			"name": "Attribute",
			"scope": [
				"meta.attribute",
			],
			"settings": {
				"foreground": "#5affe6"
			}
		},
		{
			"name": "Attribute string",
			"scope": [
				"meta.attribute string.quoted",
			],
			"settings": {
				"foreground": "#f1bdff"
			}
		}
	]
}